
public class Sample 
{

   void addProduct(int pid)
   {
	   System.out.println("This is addProduct..");
   }
   void viewProduct()
   {
	   System.out.println("This is viewProduct..");
   }
	
	public static void main(String[] args)
	{
		
		Sample s1=new Sample();
		s1.addProduct();
		s1.viewProduct();

	}

}
