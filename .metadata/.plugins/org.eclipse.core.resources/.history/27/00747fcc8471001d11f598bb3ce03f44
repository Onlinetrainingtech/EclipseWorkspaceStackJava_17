
public class Sample 
{

   void addProduct(int pid)
   {
	   System.out.println("This is addProduct.."+pid);
   }
   void viewProduct(String pname)
   {
	   System.out.println("This is viewProduct.."+pname);
   }
	
	public static void main(String[] args)
	{
		
		Sample s1=new Sample();
		s1.addProduct();
		s1.viewProduct();

	}

}
